<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.baizhi.xzy.dao.OrderDao">
	<!-- 添加订单||订单入库 -->
	<insert id="insertO">
		insert into d_order values(#{id},#{order_number},#{order_times},#{total_price},#{status},#{name},#{address},#{user_id},#{addr_id})
	</insert>
	<!-- 后台：查询所有订单 -->
	<select id="selectALLO" resultType="Order">
		select id,order_number,order_times,total_price,status,name,address,user_id,addr_id from d_order
	</select>
</mapper>
